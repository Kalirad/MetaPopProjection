	pages = {115-155},
	publisher = {Academic Press},
	series = {Advances in Genetics},
	title = {Evolutionary Significance of Phenotypic Plasticity in Plants},
	url = {https://www.sciencedirect.com/science/article/pii/S0065266008600486},
	volume = {13},
	year = {1965},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0065266008600486},

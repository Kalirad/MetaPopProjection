	journal = {Trends in Ecology \& Evolution},
	keywords = {phenotype plasticity, costs, constraints, adaptation, genotype-by-environment interactions},
	number = {2},
	pages = {77-81},
	title = {Costs and limits of phenotypic plasticity},

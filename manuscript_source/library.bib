	journal = {Evolution},
	keywords = {Developmental time, heat-shock proteins, laboratory adaptation, stress, trade-offs},
	number = {1},
	pages = {173-179},
	title = {Natural variation in the expression of the heat-shock protein HSP70 in a population of \emph{Drosophila Melanogaster} and its correlation with tolerance of ecologically relevant thermal stress},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1558-5646.1997.tb02398.x},
	volume = {51},
	year = {1997},
